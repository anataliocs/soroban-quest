{
  "name": "Soroban Quest Dev Container",
  "dockerFile": "Dockerfile",
  "context": "..",
  "features": {
    "ghcr.io/devcontainers/features/rust": 1,
    "ghcr.io/devcontainers-extra/features/vscode-cli:1": {},
    "ghcr.io/devcontainers-extra/features/curl-homebrew:1": {}
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        "README.md",
      ]
    },
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "terminal.integrated.defaultProfile.linux": "bash",
        "@ext:LeonardSSH.vscord.app.id": "1309255291048558632",
        "@ext:LeonardSSH.vscord.app.name": "Visual Studio Code",
        "@ext:LeonardSSH.vscord.enable": true,
        "workbench.startupEditor": "readme",
        "vscode.commands.executeCommand": true,
        "github.codespaces.devcontainerChangedNotificationStyle": "modal"
      },
      "extensions": [
        "streetsidesoftware.code-spell-checker",
        "rust-lang.rust-analyzer",
        "LeonardSSH.vscord"
      ]
    }
  },
  "onCreateCommand": "brew install stellar-cli",
  "initializeCommand": "",
  "postCreateCommand": "rustup",
  "postStartCommand": "",
  "postAttachCommand": "",
  "remoteUser": "vscode",
}